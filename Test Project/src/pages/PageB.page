<apex:page controller="wizard_class">
<apex:form >
    <apex:pageBlock title="Enter Contact Details">
        <apex:pageBlockSection collapsible="false" columns="1">
            <apex:inputField value="{!c.FirstName}"/>
            <apex:inputField value="{!c.LastName}"/>
            <apex:inputField value="{!c.Email}"/>
            <apex:inputField value="{!c.Phone}"/>
            <apex:inputField value="{!c.Fax}"/>
        </apex:pageBlockSection>
        
        <apex:pageBlockButtons >
            <apex:commandButton value="Previous Page" action="{!pageA}"/>
            <apex:commandButton value="Next Page" action="{!pageC}"/>
        </apex:pageBlockButtons>
    </apex:pageBlock>
</apex:form>
</apex:page>